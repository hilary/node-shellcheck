{
    "name": "node-shellcheck",
    "version": "0.4.8",
    "description": "Wrapper to download shellcheck",
    "bin": {
        "shellcheck": "bin/shellcheck",
        "shellcheck.bat": "bin/shellcheck.bat"
    },
    "main": "src/shellcheck.js",
    "files": [
        "src/install.js",
        "src/shellcheck.js",
        "bin/shellcheck",
        "bin/shellcheck.bat",
        "README.md"
    ],
    "scripts": {
        "provision": "npm install -g rimraf node-pre-gyp",
        "postinstall": "node src/install.js",
        "postversion": "git push --follow-tags",
        "test": "cd tests && yarn install",
        "build": "eslint src/",
        "lint": "eslint src/",
        "fix": "eslint --fix src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joelvaneenwyk/shellcheck.git"
    },
    "keywords": [
        "shellcheck",
        "shell"
    ],
    "author": "Gunar C. Gessner (wrapper only)",
    "contributors": [
        {
            "name": "Joel Van Eenwyk",
            "email": "joel.vaneenwyk@gmail.com",
            "url": "http://www.joelvaneenwyk.com"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/joelvaneenwyk/shellcheck/issues"
    },
    "homepage": "https://github.com/joelvaneenwyk/shellcheck#readme",
    "dependencies": {
        "decompress": "^4.2.1",
        "decompress-tar": "^4.1.1",
        "decompress-tarxz": "^3.0.0",
        "mkdirp": "^1.0.4",
        "shelljs": "^0.8.4",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "eslint": "^7.6.0",
        "eslint-config-google": "^0.14.0",
        "prettier": "^2.0.5"
    }
}
