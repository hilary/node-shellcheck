{
    "name": "node-shellcheck",
    "version": "0.5.4",
    "description": "Wrapper to download shellcheck",
    "bin": {
        "shellcheck": "bin/shellcheck.js"
    },
    "main": "bin/shellcheck.js",
    "types": "bin/shellcheck.d.ts",
    "files": [
        "bin/*.js",
        "README.md",
        "LICENSE",
        "package.json"
    ],
    "scripts": {
        "provision": "yarn install -g rimraf node-pre-gyp",
        "postinstall": "node bin/install.js",
        "postversion": "git push --follow-tags",
        "test": "cd tests && yarn install",
        "build": "tsc --project tsconfig.production.json && eslint src/** && node bin/install.js && cd tests && yarn install --check-files",
        "lint": "eslint src/**",
        "fix": "eslint --fix src/**"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joelvaneenwyk/shellcheck.git"
    },
    "keywords": [
        "shellcheck",
        "shell"
    ],
    "author": "Gunar C. Gessner (wrapper only)",
    "contributors": [
        {
            "name": "Joel Van Eenwyk",
            "email": "joel.vaneenwyk@gmail.com",
            "url": "http://www.joelvaneenwyk.com"
        }
    ],
    "readmeFilename": "README.md",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/joelvaneenwyk/shellcheck/issues"
    },
    "homepage": "https://github.com/joelvaneenwyk/shellcheck#readme",
    "dependencies": {
        "decompress": "^4.2.1",
        "decompress-tar": "^4.1.1",
        "decompress-tarxz": "^3.0.0",
        "mkdirp": "^1.0.4",
        "shelljs": "^0.8.4",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@types/decompress": "^4.2.3",
        "@types/mkdirp": "^1.0.1",
        "@types/node": "^14.0.27",
        "@types/shelljs": "^0.8.8",
        "@types/yauzl": "^2.9.1",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
        "@typescript-eslint/parser": "^3.9.0",
        "eslint": "^7.6.0",
        "eslint-config-google": "^0.14.0",
        "prettier": "^2.0.5",
        "typescript": "^3.9.7"
    }
}
