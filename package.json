{
    "name": "node-shellcheck",
    "version": "0.4.5",
    "description": "Wrapper to download shellcheck",
    "bin": {
        "shellcheck": "node src/shellcheck.js"
    },
    "files": [
        "src/download.js",
        "src/shellcheck.js",
        "README.md"
    ],
    "scripts": {
        "postinstall": "node src/download.js",
        "postversion": "git push --follow-tags",
        "test": "node src/shellcheck.js tests/example.bash",
        "lint": "npx eslint src/",
        "fix": "npx eslint --fix src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gunar/shellcheck.git"
    },
    "keywords": [
        "shellcheck",
        "shell"
    ],
    "author": "Gunar C. Gessner (wrapper only)",
    "contributors": [
        {
            "name": "Joel Van Eenwyk",
